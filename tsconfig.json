{
  "extends": "./node_modules/electron-webpack/tsconfig-base.json",
  "compilerOptions": {
    "noImplicitAny": true,
    "outDir": "dist",
    "baseUrl": ".",
    "paths": {
      "@/*": [ "src/*" ],
      "@root/*": [ "./*" ],
      "@common/*": [ "src/common/*" ]
    },
    "typeRoots": [
      "node_modules/@types",
      "src/types",
    ]
  },
  "include": [
    "src/**/*"
  ]
}
