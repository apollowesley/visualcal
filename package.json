{
  "name": "visualcal-electron",
  "productName": "IndySoft VisualCal",
  "version": "0.1.0",
  "description": "IndySoft VisualCal",
  "private": true,
  "main": "dist/main/index.js",
  "scripts": {
    "dist": "electron-builder",
    "pack": "electron-builder --dir",
    "release": "electron-builder build -p never",
    "postinstall": "electron-builder install-app-deps",
    "build": "tsc",
    "watch": "tsc -w",
    "clean:dist": "rm -rf dist",
    "start": "npm run clean:dist && npm run build && electron --disable-gpu dist/main/index.js",
    "update:all": "npm-update-all",
    "rebuild": "rm -rf node_modules && npm install && electron-rebuild --force",
    "copynodes:packagejson": "cp src/nodes/package.json dist/nodes",
    "copynodes:html": "cp src/nodes/*.html dist/nodes",
    "copynodes:examples": "cp -R src/nodes/examples dist/nodes",
    "copynodes:icons": "cp -R src/nodes/icons dist/nodes",
    "copynodes:locales": "cp -R src/nodes/locales dist/nodes",
    "copynodes": "npm run copynodes:packagejson && npm run copynodes:html && npm run copynodes:examples && npm run copynodes:icons && npm run copynodes:locales"
  },
  "repository": {
    "type": "git",
    "url": "https://indysoftdev@dev.azure.com/indysoftdev/VisualCal/_git/visualcal-electron"
  },
  "dependencies": {
    "@node-red-contrib-themes/midnight-red": "^1.3.1",
    "@schemastore/package": "0.0.5",
    "connect-history-api-fallback": "^1.6.0",
    "d3": "^5.16.0",
    "denque": "^1.4.1",
    "directory-tree": "^2.2.4",
    "electron-is-dev": "^1.2.0",
    "express": "^4.17.1",
    "grapesjs": "^0.16.18",
    "grapesjs-custom-code": "^0.1.3",
    "grapesjs-lory-slider": "^0.1.5",
    "grapesjs-parser-postcss": "^0.1.1",
    "grapesjs-preset-webpage": "^0.1.11",
    "grapesjs-style-bg": "^1.0.3",
    "grapesjs-tabs": "^0.2.2",
    "grapesjs-tooltip": "^0.1.5",
    "grapesjs-touch": "^0.1.1",
    "grapesjs-tui-image-editor": "^0.1.3",
    "grapesjs-typed": "^1.0.5",
    "is-utf8": "^0.2.1",
    "lodash": "^4.17.15",
    "materialize-css": "^1.0.0",
    "mathjs": "^7.0.2",
    "module-alias": "^2.2.2",
    "moment": "^2.27.0",
    "node-red": "^1.0.6",
    "node-red-contrib-play-audio": "^2.5.0",
    "node-red-contrib-web-worldmap": "^2.3.13",
    "node-red-dashboard": "^2.22.1",
    "node-red-node-base64": "^0.2.1",
    "node-red-node-geofence": "^0.1.2",
    "node-red-node-random": "^0.2.0",
    "node-red-node-serialport": "^0.10.3",
    "node-red-node-smooth": "^0.1.2",
    "serialport": "^9.0.0",
    "source-map-support": "^0.5.19",
    "tabulator-tables": "^4.7.0",
    "toastr": "^2.1.4",
    "vue": "^2.6.11",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/connect-history-api-fallback": "^1.3.3",
    "@types/d3": "^5.7.2",
    "@types/events": "^3.0.0",
    "@types/express": "^4.17.6",
    "@types/lodash": "^4.14.156",
    "@types/materialize-css": "^1.0.8",
    "@types/mathjs": "^6.0.5",
    "@types/node-red": "^0.20.1",
    "@types/serialport": "^8.0.1",
    "@types/tabulator-tables": "^4.6.7",
    "@types/toastr": "^2.1.38",
    "electron": "^10.0.0-beta.4",
    "electron-builder": "^22.7.0",
    "electron-notarize": "^1.0.0",
    "electron-rebuild": "^1.11.0",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "vue-devtools": "^5.1.3"
  },
  "keywords": [
    "electron",
    "indysoft",
    "visualcal"
  ],
  "author": {
    "name": "Scott Page",
    "email": "scottpageusmc@gmail.com"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://dev.azure.com/indysoftdev/VisualCal/_backlogs/backlog/VisualCal%20Team/Epics"
  },
  "homepage": "https://dev.azure.com/indysoftdev/VisualCal/_git/visualcal-electron",
  "build": {
    "appId": "com.indysoft.visualcal",
    "productName": "VisualCal Electron",
    "copyright": "Copyright Â© 2020 IndySoft",
    "asar": false,
    "files": [
      "dist/**/*",
      "public/**/*",
      "bootstrap-studio/exported/**/*",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": "dmg",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist"
    },
    "dmg": {
      "sign": false
    }
  },
  "visualcal": {
    "drivers": {
      "interfaces": [
        {
          "displayName": "Prologix GPIB TCP",
          "className": "PrologixGpibTcpInterface",
          "path": "dist/communication-interfaces/prologix/PrologixGpibTcpInterface.js"
        },
        {
          "displayName": "Prologix GPIB USB",
          "className": "PrologixGpibUsbInterface",
          "path": "dist/communication-interfaces/prologix/PrologixGpibUsbInterface.js"
        },
        {
          "displayName": "Emulated",
          "className": "EmulatedCommunicationInterface",
          "path": "dist/communication-interfaces/EmulatedCommunicationInterface.js"
        }
      ],
      "devices": [
        {
          "displayName": "Fluke 45",
          "className": "Fluke45",
          "manufacturer": "Fluke",
          "model": "45",
          "categories": [
            "Digital Multimeter"
          ],
          "path": "dist/devices/digital-multimeters/Fluke45.js"
        },
        {
          "displayName": "Keysight 34401A",
          "className": "Keysight34401A",
          "manufacturer": "Keysight",
          "model": "34401A",
          "categories": [
            "Digital Multimeter"
          ],
          "path": "dist/devices/digital-multimeters/Keysight34401A.js"
        },
        {
          "displayName": "Fluke 5522A",
          "className": "Fluke5522A",
          "manufacturer": "Fluke",
          "model": "5522A",
          "categories": [
            "Multi-Product Calibrator"
          ],
          "path": "dist/devices/multi-product-calibrators/Fluke5522ADevice.js"
        }
      ]
    }
  }
}
