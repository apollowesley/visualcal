{
    "name": "visualcal-electron",
    "version": "1.0.6",
    "description": "Electron Node-RED application starter kit for development",
    "private": true,
    "main": "dist/main/main.js",
    "scripts": {
        "dev": "npm run compile:renderers && electron-webpack dev",
        "dev:nocompile": "electron-webpack dev",
        "compile": "electron-webpack",
        "compile:renderers": "tsc -p src/renderers  && rm -rf html/main",
        "dist": "npm run compile && electron-builder",
        "pack": "electron-builder --dir",
        "release": "rm -rf dist && npm run compile:renderers && npm run compile && electron-builder build -p never",
        "postinstall": "electron-builder install-app-deps",
        "build": "tsc && electron-webpack",
        "watch": "tsc -w",
        "start": "npm run compile && electron --disable-gpu dist/main/main.js",
        "test": "echo \"Error: no test specified\" && exit 1",
        "clean-dist": "rm -rf dist",
        "upgrade": "npm-update-all",
        "rebuild": "rm -rf node_modules && npm install && electron-rebuild --force && npm install",
        "upgrade-rebuild": "npm run upgrade && npm run rebuild"
    },
    "repository": {
        "type": "git",
        "url": "https://indysoftdev@dev.azure.com/indysoftdev/VisualCal/_git/visualcal-electron"
    },
    "dependencies": {
        "@node-red-contrib-themes/midnight-red": "^1.3.1",
        "@schemastore/package": "0.0.5",
        "d3": "^5.16.0",
        "directory-tree": "^2.2.4",
        "electron-is-dev": "^1.2.0",
        "express": "^4.17.1",
        "grapesjs": "^0.16.12",
        "grapesjs-custom-code": "^0.1.2",
        "grapesjs-lory-slider": "^0.1.5",
        "grapesjs-parser-postcss": "^0.1.1",
        "grapesjs-preset-webpage": "^0.1.11",
        "grapesjs-style-bg": "^1.0.3",
        "grapesjs-tabs": "^0.2.2",
        "grapesjs-tooltip": "^0.1.5",
        "grapesjs-touch": "^0.1.1",
        "grapesjs-tui-image-editor": "^0.1.3",
        "grapesjs-typed": "^1.0.5",
        "materialize-css": "^1.0.0-rc.2",
        "moment": "^2.26.0",
        "node-red": "^1.0.6",
        "node-red-contrib-play-audio": "^2.5.0",
        "node-red-contrib-web-worldmap": "^2.3.13",
        "node-red-dashboard": "^2.22.1",
        "node-red-node-base64": "^0.2.1",
        "node-red-node-geofence": "^0.1.2",
        "node-red-node-random": "^0.1.4",
        "node-red-node-serialport": "^0.10.2",
        "node-red-node-smooth": "^0.1.2",
        "source-map-support": "^0.5.19",
        "tabulator-tables": "^4.6.3",
        "toastr": "^2.1.4",
        "winston": "^3.2.1"
    },
    "devDependencies": {
        "@types/d3": "^5.7.2",
        "@types/electron": "^1.6.10",
        "@types/express": "^4.17.6",
        "@types/materialize-css": "^1.0.8",
        "@types/moment": "^2.13.0",
        "@types/node-red": "^0.20.1",
        "@types/tabulator-tables": "^4.6.7",
        "@types/toastr": "^2.1.38",
        "@types/winston": "^2.4.4",
        "electron": "^9.0.4",
        "electron-builder": "^22.7.0",
        "electron-rebuild": "^1.11.0",
        "electron-webpack": "^2.8.2",
        "electron-webpack-ts": "^4.0.1",
        "ts-loader": "^7.0.5",
        "typescript": "^3.9.5",
        "webpack": "^4.43.0"
    },
    "keywords": [
        "electron",
        "quick start",
        "node-red"
    ],
    "author": {
        "name": "Scott Page",
        "email": "scottpageusmc@gmail.com"
    },
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://dev.azure.com/indysoftdev/VisualCal/_backlogs/backlog/VisualCal%20Team/Epics"
    },
    "homepage": "https://dev.azure.com/indysoftdev/VisualCal/_git/visualcal-electron",
    "electronWebpack": {
        "main": {
            "webpackConfig": "webpack.main.additions.js"
        },
        "renderer": {
            "webpackConfig": "webpack.renderer.additions.js"
        }
    },
    "build": {
        "appId": "com.indysoft.visualcal",
        "productName": "VisualCal Electron",
        "copyright": "Copyright Â© 2020 IndySoft",
        "files": [
            "dist/**/*",
            "html/**/*",
            "nodes/**/*",
            "package.json"
        ],
        "mac": {
            "category": "public.app-category.developer-tools",
            "target": "dmg"
        }
    }
}
