{
    "name": "electron_node_red",
    "version": "1.0.6",
    "description": "Electron Node-RED application starter kit for development",
    "private": true,
    "main": "dist/src/main.js",
    "_moduleAliases": {
        "@": "dist/src",
        "@root": ".",
        "@menu": "dist/src/menu",
        "@shared": "dist/src/shared"
    },
    "NRelectron": {
        "productName": "VisualCal",
        "editable": true,
        "allowLoadSave": false,
        "showMap": false,
        "kioskMode": false,
        "addNodes": true,
        "flowFile": "/Users/scottpage/Documents/testing/electron-node-red/electron-node-red/electronflow.json",
        "start": "dashboard"
    },
    "scripts": {
        "pack": "electron-builder --dir",
        "dist": "electron-builder",
        "release": "electron-builder build -p always",
        "postinstall": "electron-builder install-app-deps",
        "build": "tsc",
        "watch": "tsc -w",
        "start": "npm run build && electron --disable-gpu dist/src/main.js",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/scottpage/electron-node-red.git"
    },
    "dependencies": {
        "@node-red-contrib-themes/midnight-red": "^1.3.1",
        "@schemastore/package": "0.0.5",
        "d3": "^5.16.0",
        "electron-is-dev": "^1.2.0",
        "express": "^4.17.1",
        "module-alias": "^2.2.2",
        "node-red": "1.0.6",
        "node-red-contrib-play-audio": "^2.5.0",
        "node-red-contrib-web-worldmap": "^2.3.13",
        "node-red-dashboard": "^2.22.1",
        "node-red-node-base64": "^0.2.1",
        "node-red-node-geofence": "^0.1.2",
        "node-red-node-random": "^0.1.4",
        "node-red-node-serialport": "^0.10.2",
        "node-red-node-smooth": "^0.1.2"
    },
    "devDependencies": {
        "@types/d3": "^5.7.2",
        "@types/electron": "^1.6.10",
        "@types/express": "^4.17.6",
        "@types/node-red": "^0.20.1",
        "electron": "~7.2.1",
        "electron-builder": "^22.5.1",
        "typescript": "^3.9.5"
    },
    "keywords": [
        "electron",
        "quick start",
        "node-red"
    ],
    "author": {
        "name": "Scott Page",
        "email": "scottpageusmc@gmail.com"
    },
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/scottpage/electron-node-red/issues"
    },
    "homepage": "https://github.com/scottpage/electron-node-red#readme",
    "build": {
        "appId": "com.electron.node-red",
        "productName": "Node-RED Electron",
        "copyright": "Copyright Â© 2018,2020 Scott Page",
        "mac": {
            "category": "public.app-category.developer-tools",
            "target": "dmg"
        },
        "linux": {
            "target": [
                "rpm",
                {
                    "target": "deb",
                    "arch": [
                        "x64",
                        "armv7l"
                    ]
                }
            ],
            "synopsis": "Node-RED Low Code development",
            "category": "Development"
        },
        "win": {
            "target": [
                {
                    "target": "msi",
                    "arch": [
                        "x64"
                    ]
                }
            ]
        }
    }
}
