{
  "name": "indysoft-visualcal",
  "productName": "VisualCal",
  "version": "0.1.0-alpha.63",
  "description": "VisualCal",
  "private": true,
  "main": "dist/main/index.js",
  "scripts": {
    "dist": "electron-builder",
    "pack": "electron-builder --dir",
    "release": "electron-builder build -p never",
    "release:publish": "electron-builder build -p always",
    "postinstall": "electron-builder install-app-deps && patch-package",
    "build": "tsc",
    "watch": "tsc -w",
    "clean:dist": "rm -rf dist",
    "start": "npm run clean:dist && npm run build && electron --disable-gpu dist/main/index.js",
    "update:all": "npm-update-all",
    "rebuild": "rm -rf node_modules && npm install && electron-rebuild --force",
    "copydrivers:packagejson": "cp src/drivers/package.json dist/drivers",
    "copynodes:packagejson": "cp src/nodes/package.json dist/nodes",
    "copynodes:html": "cp src/nodes/*.html dist/nodes",
    "copynodes:examples": "cp -R src/nodes/examples dist/nodes",
    "copynodes:icons": "cp -R src/nodes/icons dist/nodes",
    "copynodes:locales": "cp -R src/nodes/locales dist/nodes",
    "copynodes": "npm run copynodes:packagejson && npm run copynodes:html && npm run copynodes:examples && npm run copynodes:icons && npm run copynodes:locales",
    "fix-electron-diff-updater": "cp bug-fixes/electron-differential-updater/AppUpdater.js node_modules/@imjs/electron-differential-updater/out/AppUpdater.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/scottpageindysoft/visualcal"
  },
  "dependencies": {
    "@imjs/electron-differential-updater": "^5.0.1",
    "@node-red-contrib-themes/midnight-red": "^1.4.4",
    "@schemastore/package": "0.0.6",
    "aedes": "^0.42.6",
    "d3": "^6.1.1",
    "denque": "^1.4.1",
    "directory-tree": "^2.2.4",
    "electron-cfg": "^1.2.5",
    "electron-ipc-log": "^3.0.1",
    "electron-is-dev": "^1.2.0",
    "electron-log": "^4.2.4",
    "electron-store": "^6.0.0",
    "express": "^4.17.1",
    "grapesjs": "^0.16.22",
    "grapesjs-custom-code": "^0.1.3",
    "grapesjs-lory-slider": "^0.1.5",
    "grapesjs-parser-postcss": "^0.1.1",
    "grapesjs-preset-webpage": "^0.1.11",
    "grapesjs-style-bg": "^1.0.3",
    "grapesjs-tabs": "^0.2.2",
    "grapesjs-tooltip": "^0.1.5",
    "grapesjs-touch": "^0.1.1",
    "grapesjs-tui-image-editor": "^0.1.3",
    "grapesjs-typed": "^1.0.5",
    "is-utf8": "^0.2.1",
    "jquery": "^3.5.1",
    "lodash": "^4.17.20",
    "materialize-css": "^1.0.0",
    "mathjs": "^7.2.0",
    "moment": "^2.28.0",
    "mousetrap": "^1.6.5",
    "node-red": "^1.1.3",
    "node-red-node-serialport": "^0.11.0",
    "sanitize-filename": "^1.6.3",
    "serialport": "^9.0.1",
    "source-map-support": "^0.5.19",
    "tabulator-tables": "^4.8.1",
    "tingodb": "^0.6.1",
    "tiny-typed-emitter": "^2.0.2",
    "udp-discovery": "^2.0.1",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@types/bootstrap": "^4.5.0",
    "@types/d3": "^5.7.2",
    "@types/events": "^3.0.0",
    "@types/express": "^4.17.8",
    "@types/js-quantities": "^1.6.2",
    "@types/lodash": "^4.14.161",
    "@types/materialize-css": "^1.0.8",
    "@types/mathjs": "^6.0.5",
    "@types/mongodb": "^3.5.27",
    "@types/node-red": "~0.20.2",
    "@types/serialport": "^8.0.1",
    "@types/tabulator-tables": "^4.8.0",
    "@types/toastr": "^2.1.38",
    "@types/uuid": "^8.3.0",
    "@types/when": "^2.4.33",
    "electron": "^10.1.2",
    "electron-builder": "^22.8.1",
    "electron-notarize": "^1.0.0",
    "electron-rebuild": "^2.0.3",
    "patch-package": "^6.2.2",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2",
    "vue-devtools": "^5.1.4"
  },
  "keywords": [
    "electron",
    "indysoft",
    "visualcal"
  ],
  "author": {
    "name": "Scott Page",
    "email": "scottpageusmc@gmail.com"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://dev.azure.com/indysoftdev/VisualCal/_boards/board/t/VisualCal%20Team/Epics/"
  },
  "homepage": "https://github.com/scottpageindysoft/visualcal",
  "build": {
    "appId": "com.indysoft.visualcal",
    "productName": "VisualCal",
    "copyright": "Copyright Â© 2020 IndySoft",
    "asar": false,
    "npmRebuild": false,
    "files": [
      "dist/**/*",
      "public/**/*",
      "demo/**/*",
      "common/**/*",
      "package.json"
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "publish": [
        {
          "provider": "github",
          "releaseType": "release",
          "publishAutoUpdate": true,
          "useAppSupportCache": true
        }
      ]
    },
    "nsis": {
      "artifactName": "${productName}-Setup-${version}.${ext}",
      "oneClick": true,
      "perMachine": false,
      "allowToChangeInstallationDirectory": false,
      "runAfterFinish": true,
      "deleteAppDataOnUninstall": false
    }
  },
  "visualcal": {
    "drivers": {
      "interfaces": [
        {
          "displayName": "Prologix GPIB TCP",
          "className": "PrologixGpibTcpInterface",
          "path": "dist/communication-interfaces/prologix/PrologixGpibTcpInterface.js"
        },
        {
          "displayName": "Prologix GPIB USB",
          "className": "PrologixGpibUsbInterface",
          "path": "dist/communication-interfaces/prologix/PrologixGpibUsbInterface.js"
        },
        {
          "displayName": "Emulated",
          "className": "EmulatedCommunicationInterface",
          "path": "dist/communication-interfaces/EmulatedCommunicationInterface.js"
        }
      ],
      "devices": [
        {
          "displayName": "Fluke 45",
          "className": "Fluke45",
          "manufacturer": "Fluke",
          "model": "45",
          "categories": [
            "Digital Multimeter"
          ],
          "path": "dist/devices/digital-multimeters/Fluke45.js"
        },
        {
          "displayName": "Keysight 34401A",
          "className": "Keysight34401A",
          "manufacturer": "Keysight",
          "model": "34401A",
          "categories": [
            "Digital Multimeter"
          ],
          "path": "dist/devices/digital-multimeters/Keysight34401A.js"
        },
        {
          "displayName": "Fluke 5522A",
          "className": "Fluke5522A",
          "manufacturer": "Fluke",
          "model": "5522A",
          "categories": [
            "Multi-Product Calibrator"
          ],
          "path": "dist/devices/multi-product-calibrators/Fluke5522ADevice.js"
        }
      ]
    }
  }
}
