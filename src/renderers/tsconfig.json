{
  "compilerOptions": {
    "target": "ES5",
    "noImplicitAny": true,
    "sourceMap": true,
    "allowJs": false,
    "experimentalDecorators": true,
    "moduleResolution": "Node",
    "outDir": "../../html",
    "baseUrl": ".",
    "esModuleInterop": true,
    "typeRoots": [
      "../../node_modules/@types",
      "../types"
    ]
  },
  "include": [
    "./**/*.ts",
    "../types/**/*"
  ]
}
