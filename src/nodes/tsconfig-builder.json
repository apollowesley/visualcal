{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "noImplicitAny": true,
    "sourceMap": true,
    "strict": true,
    "experimentalDecorators": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "outDir": "../../nodes-build",
    "baseUrl": ".",
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "include": [
    "builder.ts"
  ]
}
